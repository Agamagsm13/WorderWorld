<?xml version="1.0" encoding="utf-8"?>


<resources>

    <declare-styleable name="KeyboardViewStyle">
        <attr name="keyboard_backgroundColor" format="color" />
    </declare-styleable>

    <declare-styleable name="KeyViewStyle">
        <attr name="key_state" format="integer" />
        <attr name="key_text" format="string" />
    </declare-styleable>

    <declare-styleable name="WordViewStyle">
        <attr name="word_length" format="integer" />
    </declare-styleable>

    <declare-styleable name="LetterViewStyle">
        <attr name="letter_state" format="integer" />
    </declare-styleable>
</resources>